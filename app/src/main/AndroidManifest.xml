<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="irs.instantreplysystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImportActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".CheckInActivity" />
        <activity android:name=".ReplyActivity" />
        <activity android:name=".RouteMapActivity" />
        <activity android:name=".ShopMapActivity" />
        <activity android:name=".BarCodeActivity" />
        <activity android:name=".SignActivity"></activity>
    </application>

</manifest>